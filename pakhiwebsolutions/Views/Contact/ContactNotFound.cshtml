@model int
@{
    ViewBag.Title = "404 Error";
}
<div class="alert alert-danger mt=1 mb=1">
    <h4>404 Not Found Error:</h4>
    <hr />
    <h5>
        Contact with Contact Id=@Model cannot be found
    </h5>
</div>
<a asp-action="index" asp-controller="Contact" class="btn btn-outline-success" style="width:auto">
    Click here to fill feedback 
</a>
